{
  "name": "phi-react",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/client": "^3.5.8",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.0",
    "@types/node": "^16.11.21",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "graphql": "^15.8.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-dom-confetti": "^0.2.0",
    "react-scripts": "5.0.0",
    "subscriptions-transport-ws": "^0.11.0",
    "typescript": "^4.5.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start:js": "react-scripts start",
    "build:js": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build": "run-s build:codegen build:js",
    "start": "run-p start:codegen start:js",
    "fmt": "prettier --write './**/*.{html,css,js,jsx,ts,tsx,md,json}'",
    "gql:get-schema": "apollo client:download-schema schema.graphql",
    "build:codegen": "apollo client:codegen -c apollo.config.offline.js --target typescript",
    "start:codegen": "apollo client:codegen --watch --target typescript"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "apollo": "^2.33.9",
    "autoprefixer": "^10.4.2",
    "http-proxy-middleware": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.5",
    "postcss-import": "^14.0.2",
    "prettier": "^2.5.1",
    "tailwindcss": "^3.0.18"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5"
  }
}
